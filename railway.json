{
  "builds": [
    {
      "src": "package.json",
      "use": "@railway/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/chat",
      "dest": "/public/chat.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}